        <Label Content="Launching 9.10" HorizontalAlignment="Left" Margin="296,120,0,0" VerticalAlignment="Top" Height="46" Width="192" Foreground="#FFD0D0D0" FontSize="30" FontFamily="Segoe UI Variable Display"/>
        <Label Content="This might take a while." HorizontalAlignment="Left" Margin="240,360,0,0" VerticalAlignment="Top" Height="46" Width="304" Foreground="#FFD0D0D0" FontSize="30" FontFamily="Segoe UI Variable Display"/>
